extends layout

block content
    h1= title
    - let items = results
    - var x = 0;
    div(class="container-fluid")
      div(class="row")
      each item in items 
        div(class="col-sm-2") 
        li Doctor #{x=x+1} - Years of Experience: #{item["years_of_experience"]}, Specialty: #{item["specialty"]}, Number of Patients: #{item["number_of_patients"]}