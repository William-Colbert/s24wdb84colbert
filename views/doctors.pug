extends layout

block content
    h1= title
    - let results = [{years_of_experience:18,specialty:"Dermatology",number_of_patients:15}, {years_of_experience:6,specialty:"Family Medicine",number_of_patients:30}, {years_of_experience:11,specialty:"Oncology",number_of_patients:8}]
    - var x = 0;
    div(class="container-fluid")
      div(class="row")
      each item in results 
        div(class="col-sm-2") 
        li Doctor #{x=x+1} - Years of Experience: #{item["years_of_experience"]}, Specialty: #{item["specialty"]}, Number of Patients: #{item["number_of_patients"]}